{
    "name": "6fdigital/silverstripe-phone-field",
    "description": "A phone number field for the SilverStripe CMS with validation using libphonenumber",
    "type": "silverstripe-vendormodule",
    "homepage": "https://github.com/6fdigital/silverstripe-phone-field",
    "keywords": [
        "silverstripe",
        "form",
        "userforms",
        "phonenumber"
    ],
    "license": "Apache-2.0",
    "require": {
        "silverstripe/framework": "^4 | ^5",
        "giggsey/libphonenumber-for-php": "^8.13"
    },
    "authors": [
        {
            "name": "Marco Kernler",
            "email": "marco.kernler@6f.digital"
        }
    ],
    "support": {
        "issues": "https://github.com/6fdigital/silverstripe-phone-field/issues"
    },
    "autoload": {
        "psr-4": {
            " SixF\\PhoneField\\": "src/"
        }
    },
    "extra": {
        "installer-name": "silverstripe-phone-field",
        "expose": [
            "client/dist/",
            "client/lang/"
        ]
    }
}
